{
    "default": true,
    "no-hard-tabs": false,
    "line-length": false,
    "list-indent": true,
    "no-multiple-blanks": {
        "maximum": 1
    },
    "heading-style": {
        "style": "atx"
    },
    "blanks-around-headings": {
        "lines_below": 1,
        "lines_above": 1
    },
    "ul-style": {
        "style": "consistent"
    },
    "ul-indent": {
        "indent": 4,
        "start_indented": false
    },
    "list-marker-space": {
        "ul_single": 1,
        "ol_single": 1
    },
    "ol-prefix": {
        "style": "one_or_ordered"
    }
}
