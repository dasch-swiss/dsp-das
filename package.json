    "e2e-ci": "nx run dsp-app-e2e:e2e-ci --webdriver-update=false",
    "build": "nx run dsp-app:build",
    "build-prod": "nx run dsp-app:build:production",
    "test-ci": "nx run dsp-app:test:ci",
    "test-ci-all": "nx run-many --all --target=test --configuration=ci",
    "test-local": "nx run dsp-app:test",
    "lint-ci": "nx run dsp-app:lint",
    "lint-ci-all": "nx run-many --all --target=lint",
    "lint-local": "nx run dsp-app:lint --fix",
    "webdriver-update": "webdriver-manager update --standalone false --gecko",
    "yalc-add-lib": "rm -rf .yalc/@dasch-swiss && yalc add @dasch-swiss/dsp-js && npm install"
  },
