    "@grafana/faro-transport-otlp-http": "^1.19.0",
    "@grafana/faro-web-sdk": "^1.19.0",
    "@grafana/faro-web-tracing": "^1.19.0",
    "@ngrx/component-store": "^20.0.0",
    "@ngx-translate/core": "^15.0.0",
