{
  "name": "dsp-app",
  "version": "5.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasch-swiss/dsp-app.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-with-test-server": "ng serve --configuration=test-server",
    "lint-ci": "eslint --color -c .eslintrc.js --ext .ts ./src/app",
    "lint-local": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "e2e": "ng e2e",
    "build": "ng build",
    "build-prod": "ng build --prod=true",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test",
    "test-e2e-protractor": "ng e2e --prod=true --protractor-config=./e2e/protractor-ci.conf.js --webdriver-update=false",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "yalc-add-lib": "rm -rf .yalc/@dasch-swiss && yalc add @dasch-swiss/dsp-js && yalc add @dasch-swiss/dsp-ui && npm install"
  },
  "private": true,
  "dependencies": {
    "3d-force-graph": "^1.60.12",
    "@angular/animations": "^11.2.9",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@dasch-swiss/dsp-js": "^2.6.0",
    "@dasch-swiss/dsp-ui": "^1.7.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "5.0.0",
    "angular-split": "^4.0.0",
    "ckeditor5-custom-build": "github:dasch-swiss/ckeditor_custom_build",
    "core-js": "^3.6.5",
    "d3": "^5.15.1",
    "d3-force-3d": "^2.1.0",
    "dat.gui": "^0.7.7",
    "jdnconvertiblecalendar": "^0.0.7",
    "jdnconvertiblecalendardateadapter": "^0.0.17",
    "json2typescript": "^1.0.6",
    "jsonld": "^5.2.0",
    "moment": "^2.27.0",
    "ngx-color-picker": "^11.0.0",
    "openseadragon": "^2.4.0",
    "rxjs": "~6.5.5",
    "semver": "^6.1.1",
    "svg-overlay": "github:openseadragon/svg-overlay",
    "three": "^0.125.0",
    "three-spritetext": "^1.5.1",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
