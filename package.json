{
  "name": "dsp-app",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasch-swiss/dsp-app.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "build-prod": "ng build --prod=true",
    "build-dsp-ui": "ng build dsp-ui-lib",
    "build-dsp-js": "ng build dsp-js-lib",
    "build-dsp-libs": "ng build dsp-js-lib && ng build dsp-ui-lib",
    "init-dsp-libs": "git submodule update --init --recursive",
    "pull-dsp-libs": "git submodule update --recursive --remote",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test",
    "test-e2e-protractor": "ng e2e --prod=true --protractor-config=./e2e/protractor-ci.conf.js --webdriver-update=false",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "yalc-add-lib": "echo 'WARN: Deprecated! Use build-dsp-libs instead' && rm -rf .yalc/@dasch-swiss && yalc add @dasch-swiss/dsp-js && yalc add @dasch-swiss/dsp-ui && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@dasch-swiss/dsp-js": "^1.0.1",
    "@dasch-swiss/dsp-ui": "file:dsp_lib/dsp-ui/dist/@dasch-swiss/dsp-ui/",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "3d-force-graph": "^1.60.12",
    "angular-split": "^4.0.0",
    "ckeditor5-custom-build": "github:dasch-swiss/ckeditor_custom_build",
    "core-js": "^3.6.5",
    "d3": "^5.15.1",
    "d3-force-3d": "^2.1.0",
    "dat.gui": "^0.7.7",
    "jdnconvertiblecalendar": "^0.0.5",
    "jdnconvertiblecalendardateadapter": "^0.0.13",
    "json2typescript": "^1.0.6",
    "jsonld": "^1.1.0",
    "moment": "^2.27.0",
    "ngx-color-picker": "^10.0.1",
    "openseadragon": "^2.4.0",
    "rxjs": "~6.5.3",
    "semver": "^6.1.1",
    "svg-overlay": "github:openseadragon/svg-overlay",
    "three": "^0.118.3",
    "three-spritetext": "^1.5.1",
    "ts-md5": "^1.2.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "^0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "eslint": "^6.5.1",
    "eslint-plugin-angular": "^4.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.0.0",
    "protractor": "^7.0.0",
    "ts-node": "~8.4.1",
    "tsickle": "^0.38.1",
    "tslint": "~5.20.0",
    "typescript": "3.8.3"
  }
}
