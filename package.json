{
  "name": "dsp-app",
  "version": "9.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasch-swiss/dsp-app.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-with-test-server": "ng serve --configuration test-server",
    "lint-ci": "eslint --color -c .eslintrc.js --ext .ts ./src/app",
    "lint-local": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "e2e": "ng e2e",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test",
    "test-e2e-protractor": "ng e2e --configuration production --protractor-config=./e2e/protractor-ci.conf.js --webdriver-update=false",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "yalc-add-lib": "rm -rf .yalc/@dasch-swiss && yalc add @dasch-swiss/dsp-js && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@dasch-swiss/dsp-js": "^6.2.1",
    "@datadog/browser-rum": "^3.11.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "5.0.0",
    "angular-split": "^4.0.0",
    "ckeditor5-custom-build": "github:dasch-swiss/ckeditor_custom_build#v2.0.0",
    "core-js": "^3.21.0",
    "jdnconvertiblecalendar": "^0.0.7",
    "jdnconvertiblecalendardateadapter": "^0.0.17",
    "json2typescript": "^1.4.1",
    "jsonld": "^5.2.0",
    "moment": "^2.29.1",
    "ng2-pdf-viewer": "^7.0.2",
    "ngx-color-picker": "^11.0.0",
    "openseadragon": "^2.4.2",
    "pdfjs-dist": "^2.12.313",
    "rollbar": "^2.24.0",
    "rxjs": "~6.5.5",
    "semver": "^6.3.0",
    "ts-md5": "^1.2.11",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.16",
    "@angular-eslint/eslint-plugin": "^12.7.0",
