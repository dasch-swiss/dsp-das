{
  "name": "dsp-app",
  "version": "10.15.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasch-swiss/dsp-app.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-with-test-server": "ng serve --configuration test-server",
    "start-with-ls-test-server": "ng serve --configuration ls-test-server",
    "start-with-staging-server": "ng serve --configuration staging-server",
    "lint-ci": "eslint --color -c .eslintrc.js --ext .ts ./src/app",
    "lint-local": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "e2e": "ng e2e",
    "build": "ng build",
    "build-prod": "ng build --configuration=production",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test",
    "test-e2e-protractor": "ng e2e --configuration production --protractor-config=./e2e/protractor-ci.conf.js --webdriver-update=false",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "yalc-add-lib": "rm -rf .yalc/@dasch-swiss && yalc add @dasch-swiss/dsp-js && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/router": "^15.2.2",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@dasch-swiss/dsp-js": "^8.1.0",
    "@datadog/browser-rum": "^4.19.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "angular-split": "^14.0.0",
    "ckeditor5-custom-build": "github:dasch-swiss/ckeditor_custom_build#v2.0.0",
    "core-js": "^3.25.2",
    "jdnconvertiblecalendar": "^0.0.7",
    "jdnconvertiblecalendardateadapter": "^1.0.0",
    "json2typescript": "^1.4.1",
    "jsonld": "^6.0.0",
    "moment": "^2.29.4",
    "ng2-pdf-viewer": "^7.0.2",
    "ngx-color-picker": "^12.0.1",
    "openseadragon": "3.0.0",
    "rollbar": "^2.25.0",
    "rxjs": "~6.5.5",
    "ts-md5": "^1.3.1",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.2",
    "@angular/language-service": "^15.2.2",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.0",
    "@types/openseadragon": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "^7.0.0",
    "ts-node": "~10.9.1",
    "typescript": "4.8.3"
  }
}
