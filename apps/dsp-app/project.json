{
  "name": "dsp-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dsp-app/src",
  "projectType": "application",
  "prefix": "dsp-app",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dsp-app",
        "index": "apps/dsp-app/src/index.html",
        "main": "apps/dsp-app/src/main.ts",
        "polyfills": "apps/dsp-app/src/polyfills.ts",
        "tsConfig": "apps/dsp-app/tsconfig.app.json",
        "allowedCommonJsDependencies": [
          "json2typescript",
          "ngraph.graph",
          "ngraph.forcelayout",
          "tinycolor2",
          "ng2-pdf-viewer",
          "openseadragon"
        ],
        "assets": [
          "apps/dsp-app/src/favicon.ico",
          "apps/dsp-app/src/config",
          {
            "glob": "**/*",
            "input": "apps/dsp-app/src/assets/",
            "ignore": ["**/*.scss"],
            "output": "/assets/"
          }
        ],
        "styles": ["apps/dsp-app/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/dsp-app/src/assets/style/"]
        },
        "scripts": [
          "node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
        ]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/dsp-app/src/environments/environment.ts",
              "with": "apps/dsp-app/src/environments/environment.prod.ts"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "8mb",
              "maximumError": "8mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "9kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "test-server": {
          "fileReplacements": [
            {
              "replace": "apps/dsp-app/src/environments/environment.ts",
              "with": "apps/dsp-app/src/environments/environment.test-server.ts"
            }
          ],
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": true,
          "extractLicenses": false,
          "vendorChunk": false,
          "buildOptimizer": false
        },
        "ls-test-server": {
          "fileReplacements": [
            {
              "replace": "apps/dsp-app/src/environments/environment.ts",
              "with": "apps/dsp-app/src/environments/environment.ls-test-server.ts"
            }
          ],
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": true,
          "extractLicenses": false,
          "vendorChunk": false,
          "buildOptimizer": false
        },
        "staging-server": {
          "fileReplacements": [
            {
              "replace": "apps/dsp-app/src/environments/environment.ts",
              "with": "apps/dsp-app/src/environments/environment.staging-server.ts"
            }
          ],
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": true,
          "extractLicenses": false,
          "vendorChunk": false,
          "buildOptimizer": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "dsp-app:build"
      },
      "configurations": {
        "development": {
          "browserTarget": "dsp-app:build:development"
        },
        "production": {
          "browserTarget": "dsp-app:build:production"
        },
        "test-server": {
          "browserTarget": "dsp-app:build:test-server"
        },
        "ls-test-server": {
          "browserTarget": "dsp-app:build:ls-test-server"
        },
        "staging-server": {
          "browserTarget": "dsp-app:build:staging-server"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "dsp-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/dsp-app/**/*.ts", "apps/dsp-app/**/*.html"]
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/dsp-app/src/test.ts",
        "polyfills": "apps/dsp-app/src/polyfills.ts",
        "tsConfig": "apps/dsp-app/tsconfig.spec.json",
        "karmaConfig": "apps/dsp-app/karma.conf.js",
        "styles": ["apps/dsp-app/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/dsp-app/src/assets/style/"]
        },
        "scripts": [
          "node_modules/openseadragon/build/openseadragon/openseadragon.min.js"
        ],
        "assets": [
          "apps/dsp-app/src/favicon.ico",
          "apps/dsp-app/src/config",
          {
            "glob": "**/*",
            "input": "apps/dsp-app/src/assets/",
            "ignore": ["**/*.scss"],
            "output": "/assets/"
          }
        ]
      },
      "configurations": {
        "ci": {
          "watch": false,
          "browsers": "ChromeHeadless"
        }
      }
    }
  }
}
